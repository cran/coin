> 
> ### Example data
> f2 <- sample(gl(2, 10))
> f4 <- sample(gl(4, 5))
> o2 <- as.ordered(f2)
> o4 <- as.ordered(f4)
> n1 <- sample(1:20)
> c1 <- Surv(n1, sample(0:1, 20, replace = TRUE))
> 
